  <div class="content exchange-page">
  <input type="hidden" id="_NAMECOIN_" value="{{namecoin}}" name="">
  <input type="hidden" id="_EXCHANGE_" value="{{exchange}}" name="">
  <input type="hidden" id="_ID_" value="{{user._id}}" name="">
   <div class="container-fluid" >
      <div class="row">   
      
       <div class="col-md-12 hidden_js">

            <div class="card">
               <div class="content">


                  <h4 class="title">MARKETS</h4>
                  <div class="table-exchanges" ng-controller="CtrMarketsHome">
                    <div class="material-datatables"> 
                      <table border="1" cellpadding="0" cellspacing="0"  style="border-collapse:collapse;"  id="list-yourinvestssss" class="table table-striped table-bordered table-hover" style="width:100%;cellspacing:0" > 
                        <thead> 
                          <tr> 
                            <th>Market</th>
                            <th>Currency</th>
                            <th>Volume</th>
                            <!-- <th>% Change</th> -->
                            <th>Last Price</th>
                            <th>24Hr High</th>
                            <th>24Hr Low</th>
                            <th>Added</th>
                          </tr>
                        </thead> 
                        <tbody class="text-center">
                          {{#each data_volume}} 
                          <tr >
                            <td class="text-center"><a href="/Market/Index/{{this.MarketName}}">{{this.MarketName}}</a></td>
                            <td class="text-center">{{this.Name}}</td>
                            <td class="text-center">{{satoshi this.volume}}</td>
                            <!-- <td class="text-center">{{this.change}}</td> -->
                            <td class="text-center">{{satoshi this.last}}</td>
                            <td class="text-center">{{satoshi this.hight}}</td>
                            <td class="text-center">{{satoshi this.low}}</td>
                            <td class="text-center">{{prettifyDate this.date}}</td>
                          </tr>
                          {{/each}} 
                          
                        </tbody> 
                      </table>
                    </div>
                    <div class="clearfix"></div>
                 </div>
               </div>
            </div>
         </div>   
      </div>
   </div>
</div>
