<div class="content">
   <div class="container-fluid">

      <div class="clearfix"></div>
      
      <div class="clearfix"></div>
      <div class="row">

         <div class="col-md-12">
            <div class="card">
               
               <div class="content">
                  <div class="material-datatables">
                     <table border="0" cellspacing="5" cellpadding="5">
                        <tr>
                           <td>Start date:</td>
                           <td><input type="text" class="form-control" placeholder="month-day-year hour:min AM" id="min" name="min"></td>
                           <td>ex: 01/24/2018 12:35 AM</td>
                        </tr>
                        <tr>
                           <td>End date:</td>
                           <td><input type="text" class="form-control" placeholder="month-day-year hour:min AM" id="max" name="max"></td>
                           <td>ex: 01/24/2018 12:35 AM</td>
                        </tr>
                     </table>
                     <table id="datatables_withdraw" class="table table-striped table-no-bordered table-hover" style="width:100%;cellspacing:0">
                        <thead>
                           <tr>
                              <th>Username</th>
                              <th>Date</th>
                              <th>Price</th>
                             
                              <th>Quantity</th>
                              <th>Total</th>
                           </tr>
                        </thead>
                        <tbody>
                           {{#each history}} 
                           <tr>
                              <td>{{this.username}}</td>
                              <td>{{formatDate this.date "MM/DD/YYYY LT"}} </td>
                              <td >{{satoshi this.price}} BTC</td>
                              
                              <td>{{satoshi this.quantity}} SVA</td>
                              
                              <td>
                                {{satoshi this.total}} BTC
                              </td>
                           </tr>
                           {{/each}} 
                        </tbody>
                     </table>
                  </div>
               </div>
            </div>
         </div>
      </div>
   </div>
</div>